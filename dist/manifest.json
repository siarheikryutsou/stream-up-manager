{
  "manifest_version": 3,
  "name": "StreamUp manager",
  "version": "1.0",
  "permissions": ["tabs"],
  "host_permissions": ["https://stream-up.ru/**"],
  "content_scripts": [
    {
      "matches": ["https://stream-up.ru/*"],
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
